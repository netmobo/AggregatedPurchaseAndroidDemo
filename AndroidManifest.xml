<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="com.feefactor.samples.android.apppurchase"
	android:versionCode="3" android:versionName="3">
	<application android:icon="@drawable/icon" android:label="@string/app_name"
		android:debuggable="true" android:name="QuickstartApplication">
		<activity android:label="@string/app_name" android:name="LoginActivity">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>

		<activity android:name="SignUpActivity"></activity>
		
		<activity android:name="AccountHistoryActivity"></activity>
		<activity android:name="InputAmountActivity">
		</activity>
		<activity android:name="GoogleCheckoutActivity">
		</activity>
		<activity android:name="ViewAccountBalanceActivity"></activity>
		<activity android:name="CreditCardPaymentActivity"></activity>
		<activity android:name="PurchaseProductActivity"></activity>
		<activity android:name="PurchaseProductResultActivity"></activity>
	
	</application>
	<uses-sdk android:minSdkVersion="1" />
	
	<uses-permission android:name="android.permission.INTERNET"></uses-permission>
	<uses-permission android:name="android.permission.READ_PHONE_STATE"></uses-permission>
</manifest> 